      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "hero": {
      "type": "component",
      "repeatable": false,
      "component": "sections.hero"
    },
    "playlist": {
      "type": "component",
      "repeatable": false,
      "component": "elements.playlist"
    },
    "googleMap": {
      "type": "component",
      "repeatable": false,
      "component": "elements.gmap"
    }
  }
